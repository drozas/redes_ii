package es.urjc.escet.gsyc.html;

/**
 * 
 * Clase para generación dinámica de enlaces.
 * 
 * @author drozas
 *
 */
public class EnlaceHtml {
	
	private String enlace;
	private String textoEnlace;
	
	/**
	 * 
	 * Constructor
	 * 
	 * @param enlace
	 * @param textoEnlace
	 */
	public EnlaceHtml(String enlace, String textoEnlace)
	{
		this.enlace = enlace;
		this.textoEnlace = textoEnlace;
	}
	
	/**
	 * 
	 * Devuelve el enlace
	 * 
	 * @return
	 */
	public String getEnlace()
	{
		return this.enlace;
	}

	/**
	 * 
	 * Devuelve el texto del enlace
	 * 
	 * @return
	 */
	public String getTextoEnlace()
	{
		return this.textoEnlace;
	}
	
}
