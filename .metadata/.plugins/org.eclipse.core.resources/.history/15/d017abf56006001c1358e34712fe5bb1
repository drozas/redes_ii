package es.urjc.escet.gsyc.p2p.mensajes;

/**
 * 
 * Clase con funcionalidaes necesarioas para generar los mensajes de respuesta a la solicitud
 * de la descarga de un fichero bajo nuestro protocolo p2p
 * 
 * @author drozas
 *
 */
public class RespuestaFichero extends RespuestaP2P {
	
	//AÃ±adimos serialVerisonUID, generado con serialver
	private static final long serialVersionUID = 1648158929483834557L;

	
	public RespuestaFichero(int codigoError, String descripcion)
	{
		super(codigoError, descripcion);
	}
}