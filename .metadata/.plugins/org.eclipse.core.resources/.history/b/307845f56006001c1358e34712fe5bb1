package es.urjc.escet.gsyc.p2p.mensajes;

/**
 * 
 * Clase con funcionalidades para los mensajes de respuesta a la entrega de mensajes
 * con nuestro protocolo p2p
 * 
 * @author drozas
 *
 */
public class RespuestaEntregaMensaje extends RespuestaP2P {
	
	//AÃ±adimos serialVerisonUID, generado con serialver
	private static final long serialVersionUID = -3416575483530407186L;
	
	/**
	 * 
	 * Constructor
	 * 
	 * @param codigoError
	 * @param descripcion
	 */
	public RespuestaEntregaMensaje(int codigoError, String descripcion)
	{
		super(codigoError, descripcion);
	}
}